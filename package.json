{
  "name": "webpack-config-template",
  "version": "1.0.0",
  "description": "this is webpack5 config template",
  "main": "src/app/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "start-analyzer": "cross-env NODE_ENV=development ANALYZE=1  webpack serve",
    "start-unused": "cross-env NODE_ENV=development UNUSED=1  webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "generateAnalyzerFile": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
    "analyzer": "webpack-bundle-analyzer --port 8888 ./dist/stats.json"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yanyunchangfeng/webpack-config-template.git"
  },
  "author": "yanyunchangfeng <yanyunchangfeng@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/webpack-env": "1.16.3",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.1.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-loader": "4.1.0",
    "html-minimizer-webpack-plugin": "4.1.0",
    "html-webpack-plugin": "5.5.0",
    "image-webpack-loader": "8.1.0",
    "mini-css-extract-plugin": "2.6.0",
    "sass": "1.49.11",
    "sass-loader": "12.6.0",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "thread-loader": "3.0.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "unused-webpack-plugin": "2.4.0",
    "webpack": "5.71.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.8.0",
    "webpack-merge": "5.8.0",
    "webpackbar": "5.0.2"
  },
  "dependencies": {}
}
